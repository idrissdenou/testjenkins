pipeline{
  agent any
  stages{
    stage('Hello'){
      steps{
        sh "echo Welcome to ENE cloudTech Solutions"
      }
    }
    stage('sleep'){
      steps{
        sh 'sleep 10'
      }
    }
    stage('Hello2'){
      steps{
        sh "echo welcome to ENE cloud tech Job3"
        sh "echo welcome to ENE cloud tech Job4"
      }
    }
  }
}
